{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "BlogTitle": {
      "type": "string",
      "unique": true
    },
    "BlogContent": {
      "type": "richtext"
    },
    "FeaturedImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "blogs"
    },
    "BlogInto": {
      "type": "string",
      "unique": true
    },
    "publishedDate": {
      "type": "date",
      "unique": true
    }
  }
}
